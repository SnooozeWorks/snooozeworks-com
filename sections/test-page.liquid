{% for block in section.blocks %}
    {% case block.type %}
        {% when 'text' %}
        {% include 'page-content' %}
        {% when 'video' %}
        {% include 'video-on-page' %}
    {% endcase %}
{% endfor %}

{% schema %}
{
"blocks": [
{
  "type": "rich-text",
  "name": "Rich text",
  "settings": [ ]
},
{
  "type": "video",
  "name": "Full Width YouTube Video",
  "settings": [
    {
      "type": "text",
      "id": "videoID",
      "label": "Video ID",
      "default": "Enter Video ID Here"
    }
  ]
}
]
}
{% endschema %}